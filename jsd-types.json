  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|test)",
    "include": ["src"]
  },
  "opts": {
    "destination": ".",
    "encoding": "utf8",
    "recurse": true,
    "title": "vanillas",
