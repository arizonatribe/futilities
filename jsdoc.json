{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)",
    "include": ["src", "package.json", "README.md"]
  },
  "templates": {
    "name": "Vanillas",
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "title": "Vanillas",
    "template": "node_modules/better-docs"
  }
}
