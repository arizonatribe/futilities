{
  "name": "vanillas",
  "description": "Dependency-free, low-level JavaScript function helpers",
  "main": "src/index.js",
  "version": "1.2.1",
  "private": true,
  "license": "MIT",
  "author": "David Nunez <arizonatribe@gmail.com>",
  "types": "types.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/arizonatribe/vanillas.git"
  },
  "keywords": [
    "vanillajs",
    "javascript",
    "performance",
    "helpers",
    "vanillas",
    "function",
    "functional",
    "object",
    "compose",
    "curry",
    "composition",
    "utils",
    "utilities"
  ],
  "watch": {
    "docs": "src/*"
  },
  "scripts": {
    "dist": "npm publish build",
    "build-types": "jsdoc --configure jsd-types.json",
    "docs": "rimraf docs/* && jsdoc --configure jsdoc.json && mv docs/$npm_package_name/$npm_package_version/* docs/ && rimraf docs/$npm_package_name",
    "docs:server": "live-server --open=docs/ --watch=src,docs --port=5000",
    "start": "npm run test:watch",
    "start:docs": "npm run docs && concurrently \"npm-watch\" \"npm run docs:server\"",
    "test:watch": "nodemon test/index.js",
    "test:coverage": "nyc tape --require=@babel/register test/index.js | tap-nyc",
    "test": "tape --require=@babel/register test/index.js",
    "benchmark": "node benchmark",
    "benchmark:docs": "node benchmark --update-docs",
    "lint": "eslint src test benchmark scripts",
    "prepare": "npm run build",
    "build": "node scripts/build"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-external-helpers": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-stage-0": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.0.0",
    "babel-eslint": "8.2.1",
    "babel-plugin-add-module-exports": "1.0.0",
    "benchmark": "2.1.4",
    "better-docs": "^1.2.2",
    "chalk": "2.4.1",
    "concurrently": "^5.3.0",
    "cross-env": "5.1.4",
    "cross-spawn": "6.0.5",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "faker": "5.1.0",
    "faucet": "0.0.1",
    "fs-extra": "7.0.0",
    "fs-readdir-recursive": "1.1.0",
    "fuzzy": "0.1.3",
    "fuzzy-search": "3.2.1",
    "fuzzyjs": "4.0.4",
    "glob": "7.1.1",
    "is-promise": "^2.2.2",
    "joi": "^14.0.2",
    "jsdoc": "^3.6.3",
    "live-server": "1.2.1",
    "lodash": "^4.17.20",
    "nodemon": "1.18.7",
    "npm-watch": "0.4.0",
    "nyc": "^14.1.1",
    "ramda": "^0.25.0",
    "redux": "4.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.0",
    "spected": "^0.7.0",
    "tap-nyc": "1.0.3",
    "tape": "4.9.1",
    "tsd-jsdoc": "^2.5.0",
    "tui-jsdoc-template": "1.2.2",
    "yup": "^0.26.6"
  }
}
