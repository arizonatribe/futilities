{
  "name": "vanillas",
  "version": "0.1.1",
  "description": "Dependency-free, low-level JavaScript function helpers",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "browser": "dist/vanillas.js",
  "scripts": {
    "docs": "jsdoc --configure jsdoc.json",
    "start": "npm run test:watch",
    "prestart:docs": "npm run docs",
    "start:docs": "serve docs",
    "test:watch": "nodemon test/index.js",
    "test:coverage": "nyc tape --require=@babel/register test/index.js | tap-nyc",
    "test": "tape --require=@babel/register test/index.js | faucet",
    "benchmark": "node benchmark",
    "benchmark:docs": "node benchmark --update-docs",
    "clean": "rimraf dist es lib",
    "lint": "eslint src test",
    "prepublish": "npm run build",
    "prebuild": "npm run clean && npm run lint && npm test",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development rollup -c",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c",
    "build": "npm run build:es && npm run build:cjs && npm run build:umd && npm run build:umd:min"
  },
  "keywords": [
    "helpers",
    "vanillas",
    "function",
    "functional",
    "object",
    "compose",
    "curry",
    "composition",
    "utils",
    "utilities"
  ],
  "author": "David Nunez <arizonatribe@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arizonatribe/vanillas.git"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-external-helpers": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-stage-0": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.0.0",
    "babel-eslint": "8.2.1",
    "babel-register": "6.26.0",
    "babel-tape-runner": "3.0.0",
    "benchmark": "2.1.4",
    "chalk": "2.4.1",
    "cross-env": "5.1.4",
    "eslint": "5.5.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "faucet": "0.0.1",
    "fs-readdir-recursive": "1.1.0",
    "glob": "7.1.1",
    "jsdoc": "3.5.5",
    "lodash": "^4.17.10",
    "nodemon": "1.11.0",
    "nyc": "13.1.0",
    "ramda": "^0.25.0",
    "redux": "4.0.0",
    "rimraf": "2.6.2",
    "rollup": "0.66.2",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-terser": "3.0.0",
    "serve": "10.0.2",
    "tap-nyc": "1.0.3",
    "tape": "4.9.1",
    "tui-jsdoc-template": "1.2.2"
  }
}
