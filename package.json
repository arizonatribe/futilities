{
  "name": "futilities",
  "version": "1.0.0",
  "description": "Dependency-free, low-level JavaScript function helpers",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "start": "nodemon test/index.js",
    "test": "node test/index.js | faucet",
    "clean": "rimraf dist es coverage",
    "lint": "eslint lib test",
    "prepublish": "npm run build",
    "prebuild": "npm run clean && npm run lint && npm test",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel lib --out-dir cjs",
    "build:es": "cross-env BABEL_ENV=es babel lib --out-dir es",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -i lib/index.js -o dist/shapey.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -i lib/index.js -o dist/shapey.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min"
  },
  "keywords": [
    "helpers",
    "futilities",
    "function",
    "functional",
    "object",
    "compose",
    "curry",
    "composition",
    "utils",
    "utilities"
  ],
  "author": "David Nunez <arizonatribe@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arizonatribe/futilities.git"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-stage-0": "7.0.0",
    "@babel/runtime": "7.0.0",
    "babel-eslint": "8.2.1",
    "babel-register": "6.26.0",
    "babel-tape-runner": "2.0.1",
    "benchmark": "2.1.4",
    "cross-env": "5.1.4",
    "eslint": "5.5.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "faucet": "0.0.1",
    "glob": "7.1.1",
    "lodash": "^4.17.10",
    "nodemon": "1.11.0",
    "ramda": "^0.25.0",
    "redux": "4.0.0",
    "rimraf": "2.6.2",
    "rollup": "0.50.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "2.0.1",
    "tape": "4.9.1",
    "uglify-es": "3.3.9"
  }
}
